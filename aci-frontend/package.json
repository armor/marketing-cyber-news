{
  "name": "aci-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:e2e": "VITE_ENABLE_MSW=false vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report tests/reports/playwright-html",
    "test:e2e:marketing": "playwright test --project=marketing --reporter=list,html",
    "test:e2e:marketing:headed": "playwright test --project=marketing --headed",
    "test:e2e:newsletter": "playwright test --project=newsletter --reporter=list,html",
    "test:e2e:regression": "playwright test --project=regression --reporter=list,html",
    "test:e2e:regression:full": "playwright test tests/e2e/*-regression.spec.ts --reporter=list,html",
    "test:e2e:smoke": "playwright test --project=smoke --reporter=list",
    "test:e2e:ci": "playwright test --project=regression --retries=2 --reporter=github,html",
    "test:performance": "playwright test tests/performance/newsletter-performance.spec.ts --reporter=list,html",
    "test:performance:api": "playwright test tests/performance/newsletter-performance.spec.ts -g \"API Performance\" --reporter=list",
    "test:performance:frontend": "playwright test tests/performance/newsletter-performance.spec.ts -g \"Frontend Performance\" --reporter=list",
    "test:performance:resources": "playwright test tests/performance/newsletter-performance.spec.ts -g \"Resource Utilization\" --reporter=list",
    "test:e2e:newsletter:complete": "playwright test tests/e2e/newsletter-complete-coverage.spec.ts --reporter=list"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.12",
    "@types/dompurify": "^3.0.5",
    "@types/react-big-calendar": "^1.16.3",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.1",
    "lucide-react": "^0.561.0",
    "msw": "^2.12.4",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-big-calendar": "^1.19.4",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.69.0",
    "react-is": "^19.2.3",
    "react-router-dom": "^7.10.1",
    "reaviz": "^16.1.0",
    "recharts": "^3.5.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.15",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jsdom": "^23.0.1",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prop-types": "^15.8.1",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.15"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
